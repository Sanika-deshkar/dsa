class Solution {
    public int maxProduct(int[] nums) {
        int maxProd=nums[0],minProd=nums[0],Prod=nums[0];
        for(int i=1;i<nums.length;i++){
            int curr=nums[i];
            if(curr<0){
                int temp=maxProd;
                maxProd=minProd;
                minProd=temp;
            }
            maxProd=Math.max(curr,maxProd*curr);
            minProd=Math.min(curr,minProd*curr);
            Prod=Math.max(Prod,maxProd);

        }
        return Prod;
}
}
